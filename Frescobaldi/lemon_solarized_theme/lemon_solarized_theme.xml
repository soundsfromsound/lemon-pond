<frescobaldi-theme name="Lemon_Solarized_v5">
 <!--
  Created by Frescobaldi 3.3
  Last Updated: Sept. 13, 2023 - Boston, MA
  Color Theme Version: 5.0
  Contact: soundsfromsound@gmail.com
  Twitter: @soundsfromsound
  Website: soundsfromsound.com
-->
  <font fontFamily="Liberation Mono" fontSize="12.0" />
  <baseColors>
    <text color="#e1e1e1" />
    <background color="#002934" />
    <selectiontext color="#c1c1c1" />
    <selectionbackground color="#11accf" />
    <current color="#0c5f74" />
    <mark color="#dddd6e" />
    <error color="#910000" />
    <search color="#c05800" />
    <match color="#01e6ff" />
    <paper color="#fffeeb" />
    <musichighlight color="#f50000" />
  </baseColors>
  <defaultStyles>
    <variable bold="False" italic="False" underline="False" textColor="#a87db8" />
    <string bold="False" italic="False" underline="False" textColor="#dda600" />
    <function bold="True" italic="False" underline="False" textColor="#d87237" />
    <escape textColor="#008080" />
    <value bold="False" italic="False" underline="False" textColor="#d0d066" />
    <comment bold="False" italic="True" underline="False" textColor="#9dbe98" />
    <error bold="False" italic="False" underline="True" textColor="#ffffff" backgroundColor="#aa0000" underlineColor="#f4ff22" />
    <keyword bold="True" italic="False" underline="False" textColor="#a0b3b3" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <command textColor="#df7114" />
      <pitch bold="False" italic="False" underline="False" textColor="#e8e8e8" />
      <octave bold="False" italic="False" underline="False" textColor="#e8e8e8" />
      <duration bold="False" italic="False" underline="False" textColor="#e8e8e8" />
      <dynamic bold="True" italic="False" underline="False" textColor="#d6d61f" />
      <check bold="True" italic="False" underline="False" textColor="#916f94" />
      <articulation bold="True" italic="False" underline="False" textColor="#c78f00" />
      <fingering bold="False" italic="False" underline="False" textColor="#b58900" />
      <stringnumber bold="False" italic="False" underline="False" textColor="#b52222" />
      <slur bold="True" italic="False" underline="False" textColor="#28aa9b" />
      <beam bold="True" italic="False" underline="False" textColor="#cb8f87" />
      <markup bold="False" textColor="#8f9f00" />
      <lyricmode textColor="#48d86a" />
      <lyrictext bold="False" italic="False" underline="False" textColor="#91da00" />
      <specifier textColor="#b58900" />
      <usercommand textColor="#00d092" />
      <delimiter textColor="#e8efef" />
      <context bold="True" italic="False" underline="False" textColor="#4eb7c7" />
      <grob bold="False" italic="False" underline="False" textColor="#e29b79" />
      <property textColor="#53ac00" />
      <variable textColor="#e78787" />
      <uservariable bold="True" italic="False" underline="False" textColor="#8e8ed3" />
      <value textColor="#eeee00" />
      <comment bold="False" underline="False" textColor="#6d8b6f" />
    </lilypond>
    <scheme>
      <scheme bold="False" italic="False" underline="False" textColor="#46d4d4" />
      <comment bold="False" underline="False" textColor="#647961" />
      <lilypond bold="True" italic="False" underline="False" textColor="#93a1a1" />
      <keyword bold="True" textColor="#d3d3d3" />
      <function bold="False" italic="False" underline="False" textColor="#2da3c3" />
      <variable bold="False" italic="False" underline="False" textColor="#d6d66a" />
      <constant bold="False" italic="False" underline="False" textColor="#eb3b96" />
      <delimiter bold="True" italic="False" underline="False" textColor="#859900" />
    </scheme>
  </allStyles>
</frescobaldi-theme>
