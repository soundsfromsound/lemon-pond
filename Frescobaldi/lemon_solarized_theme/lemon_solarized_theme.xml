<frescobaldi-theme name="Lemon-Solarized">
  <!--
  Created by Frescobaldi 3.1.1.
  Last Updated: April 6, 2020 - BOSTON
  Version: 4.20
  Contact: soundsfromsound@gmail.com
-->
  <font fontFamily="Liberation Mono" fontSize="14.0" />
  <baseColors>
    <paper color="#fffeeb" />
    <match color="#01e6ff" />
    <background color="#002934" />
    <selectionbackground color="#11accf" />
    <search color="#c05800" />
    <error color="#910000" />
    <text color="#e1e1e1" />
    <current color="#0c5f74" />
    <musichighlight color="#f50000" />
    <mark color="#dddd6e" />
    <selectiontext color="#c1c1c1" />
  </baseColors>
  <defaultStyles>
    <variable bold="False" italic="False" textColor="#a87db8" underline="False" />
    <escape textColor="#008080" />
    <comment bold="False" italic="True" textColor="#9dbe98" underline="False" />
    <string bold="False" italic="False" textColor="#dda600" underline="False" />
    <keyword bold="True" italic="False" textColor="#a0b3b3" underline="False" />
    <value bold="False" italic="False" textColor="#d0d066" underline="False" />
    <error backgroundColor="#aa0000" bold="False" italic="False" textColor="#ffffff" underline="True" underlineColor="#f4ff22" />
    <function bold="True" italic="False" textColor="#d87237" underline="False" />
  </defaultStyles>
  <allStyles>
    <scheme>
      <variable bold="False" italic="False" textColor="#d6d66a" underline="False" />
      <constant bold="False" italic="False" textColor="#bc3076" underline="False" />
      <comment bold="False" textColor="#647961" underline="False" />
      <function bold="False" italic="False" textColor="#2da3c3" underline="False" />
      <lilypond bold="True" italic="False" textColor="#93a1a1" underline="False" />
      <delimiter bold="True" italic="False" textColor="#859900" underline="False" />
      <scheme bold="False" italic="False" textColor="#46d4d4" underline="False" />
      <keyword bold="True" textColor="#d3d3d3" />
    </scheme>
    <lilypond>
      <variable textColor="#e78787" />
      <command textColor="#df7114" />
      <octave bold="False" italic="False" textColor="#e8e8e8" underline="False" />
      <markup bold="False" textColor="#8f9f00" />
      <property textColor="#53ac00" />
      <check bold="False" italic="False" textColor="#d2a1d5" underline="False" />
      <slur bold="True" italic="False" textColor="#28aa9b" underline="False" />
      <lyricmode textColor="#48d86a" />
      <beam bold="True" italic="False" textColor="#cb8f87" underline="False" />
      <dynamic bold="True" italic="False" textColor="#dae322" underline="False" />
      <delimiter textColor="#e8efef" />
      <stringnumber bold="False" italic="False" textColor="#b52222" underline="False" />
      <uservariable bold="True" italic="False" textColor="#a0a0f0" underline="False" />
      <value textColor="#eeee00" />
      <pitch bold="False" italic="False" textColor="#e8e8e8" underline="False" />
      <fingering bold="False" italic="False" textColor="#b58900" underline="False" />
      <context bold="True" italic="False" textColor="#4eb7c7" underline="False" />
      <articulation bold="True" italic="False" textColor="#c78f00" underline="False" />
      <grob bold="False" italic="False" textColor="#e29b79" underline="False" />
      <lyrictext bold="False" italic="False" textColor="#91da00" underline="False" />
      <comment bold="False" textColor="#6d8b6f" underline="False" />
      <usercommand textColor="#00d092" />
      <specifier textColor="#b58900" />
      <duration bold="False" italic="False" textColor="#e8e8e8" underline="False" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
