<frescobaldi-theme name="Lemon_Solarized_v6">
  <!--
  Created by Frescobaldi 4.0.1
  Last Updated: May 24, 2025 - Boston, MA
  Color Theme Version: 6.0
  Email: soundsfromsound@gmail.com
  Bluesky: @soundsfromsound
  Website: soundsfromsound.com
-->
  <font fontFamily="Roboto Mono" fontSize="14.0" />
  <baseColors>
    <text color="#c8c8c8" />
    <background color="#002934" />
    <selectiontext color="#d0d0d0" />
    <selectionbackground color="#13c0e3" />
    <current color="#0e728b" />
    <mark color="#dddd6e" />
    <error color="#910000" />
    <search color="#c05800" />
    <match color="#01e6ff" />
    <paper color="#fffdce" />
    <musichighlight color="#f50000" />
  </baseColors>
  <defaultStyles>
    <function bold="True" italic="False" underline="False" textColor="#d87237" />
    <value bold="True" italic="False" underline="False" textColor="#d0d066" />
    <escape textColor="#008080" />
    <error bold="True" italic="False" underline="True" textColor="#ffffff" backgroundColor="#aa0000" underlineColor="#f4ff22" />
    <keyword bold="True" italic="False" underline="False" textColor="#96a8a8" />
    <string bold="True" italic="False" underline="False" textColor="#dda600" />
    <comment bold="True" italic="True" underline="False" textColor="#9db487" />
    <variable bold="True" italic="False" underline="False" textColor="#a87db8" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <command textColor="#df7114" />
      <pitch bold="True" italic="False" underline="False" textColor="#e8e8e8" />
      <octave bold="True" italic="False" underline="False" textColor="#e8e8e8" />
      <duration bold="True" italic="False" underline="False" textColor="#e8e8e8" />
      <dynamic bold="True" italic="False" underline="False" textColor="#d6d61f" />
      <check bold="True" italic="False" underline="False" textColor="#916f94" />
      <articulation bold="True" italic="False" underline="False" textColor="#c78f00" />
      <fingering bold="True" italic="False" underline="False" textColor="#b58900" />
      <stringnumber bold="True" italic="False" underline="False" textColor="#b52222" />
      <slur bold="True" italic="False" underline="False" textColor="#28aa9b" />
      <beam bold="True" italic="False" underline="False" textColor="#cb8f87" />
      <markup bold="True" textColor="#8f9f00" />
      <lyricmode textColor="#48d86a" />
      <lyrictext bold="True" italic="False" underline="False" textColor="#91da00" />
      <specifier textColor="#b58900" />
      <usercommand textColor="#00d092" />
      <delimiter textColor="#e8efef" />
      <context bold="True" italic="False" underline="False" textColor="#4eb7c7" />
      <grob bold="True" italic="False" underline="False" textColor="#e29b79" />
      <property textColor="#53ac00" />
      <variable textColor="#e78787" />
      <uservariable bold="True" italic="False" underline="False" textColor="#8e8ed3" />
      <value textColor="#eeee00" />
      <comment bold="True" underline="False" textColor="#728f72" />
    </lilypond>
    <scheme>
      <scheme bold="True" italic="False" underline="False" textColor="#46d4d4" />
      <comment bold="True" underline="False" textColor="#647961" />
      <lilypond bold="True" italic="False" underline="False" textColor="#93a1a1" />
      <keyword bold="True" textColor="#d3d3d3" />
      <function bold="True" italic="False" underline="False" textColor="#2da3c3" />
      <variable italic="False" underline="False" textColor="#e6e672" />
      <constant bold="True" italic="False" underline="False" textColor="#eb3b96" />
      <delimiter bold="True" italic="False" underline="False" textColor="#859900" />
    </scheme>
  </allStyles>
</frescobaldi-theme>
